MEMORY
{
	ram : ORIGIN = 0x20000000, LENGTH = 65536	/* 64 MiB */
}

SECTIONS
{
	.reset	: { *(reset) } > ram
    .text	: { *(.text*) } > ram
	.rodata	: { *(.rodata*) } > ram
	.rodata	: { *(.data*) } > ram
    .rodata	: { *(.bss*) } > ram
	.stack	: {
		. = ALIGN(4096);
		. += 4096;
		c0_initialsp = .;
		. += 4096;
		c1_initialsp = .;
		. += 4096;
		c2_initialsp = .;
		. += 4096;
		c3_initialsp = .;
	} > ram
}
